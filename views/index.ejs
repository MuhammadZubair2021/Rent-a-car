  <script>
      const mainImage = document.getElementById('mainImage');
      let i = 1 ;
      function rotate(){
        mainImage.src=`/public/images/main/${i}.png`;          
          i++;
          if(i==73){
            i=1;
          }
      }
      let x = setInterval(rotate,350)
      mainImage.addEventListener('mouseover',()=>{
        clearInterval(x);        
      })
      mainImage.addEventListener('mouseout',()=>{
        x = setInterval(rotate,350);
      })
      
  </script>

    
